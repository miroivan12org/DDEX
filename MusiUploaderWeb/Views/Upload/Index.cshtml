@model MusiUploaderWeb.Models.ViewModel.UploadViewModel
@{
    ViewBag.Title = "Upload";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Upload</h2>

@using (Html.BeginForm("UploadFile", "Upload", FormMethod.Post, new { encType = "multipart/form-data" }))
{
    @Html.TextBoxFor(model => model.FileName)
    <br /><br />

    @Html.TextBoxFor(model => model.File, new { type = "file", name = "File" })<br /><br />
    <input type="submit" value="submit me" name="submitme" id="submitme" />
}

